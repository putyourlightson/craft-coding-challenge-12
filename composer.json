{
  "require": {
    "craftcms/cms": "^4.3.10",
    "putyourlightson/craft-elements-panel": "^2.0",
    "nystudio107/craft-closure": "1.0.0",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "nystudio107/craft-autocomplete": "^1.11.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "platform": {
      "php": "8.0.2"
    }
  }
}
